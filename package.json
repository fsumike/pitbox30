{
  "name": "pitbox-pwa",
  "private": true,
  "version": "3.0.0",
  "type": "module",
  "homepage": "https://pitbox.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/pitbox/pitbox-app"
  },
  "bugs": {
    "url": "https://pitbox.app/contact",
    "email": "pitboxcom@gmail.com"
  },
  "scripts": {
    "dev": "vite",
    "build": "npx cap add android || true && npx cap add ios || true && node scripts/fix-gradle.mjs && vite build && npx cap sync && node scripts/fix-ios-permissions.mjs",
    "build:ci": "npx cap add android || true && node scripts/fix-gradle.mjs && vite build && npx cap sync android",
    "build:ci:ios": "npx cap add ios || true && vite build && npx cap sync ios && node scripts/fix-ios-permissions.mjs",
    "build:ci:all": "npx cap add android || true && npx cap add ios || true && node scripts/fix-gradle.mjs && vite build && npx cap sync && node scripts/fix-ios-permissions.mjs",
    "lint": "eslint .",
    "preview": "vite preview",
    "cap:init": "cap init PitBox com.pitbox.app --web-dir=dist",
    "cap:add:android": "cap add android",
    "cap:add:ios": "cap add ios && node scripts/fix-ios-permissions.mjs",
    "cap:sync": "cap sync && node scripts/fix-ios-permissions.mjs",
    "cap:sync:ios": "cap sync ios && node scripts/fix-ios-permissions.mjs",
    "cap:sync:android": "cap sync android",
    "cap:open:android": "cap open android",
    "cap:open:ios": "cap open ios",
    "build:mobile": "npm run build && npm run cap:sync",
    "build:ios": "npm run build && npm run cap:sync:ios",
    "build:android": "npm run build && npm run cap:sync:android",
    "fix:ios-permissions": "node scripts/fix-ios-permissions.mjs",
    "fix:ios-icons": "node scripts/fix-ios-icons.mjs",
    "setup:share-target": "node scripts/setup-share-target.mjs",
    "deploy:bundle": "npm run build && npx @capawesome/cli apps:bundles:create --app-id 8251f381-4aed-4b20-ac20-a3aad250cbb8 --path dist",
    "deploy:bundle:unsigned": "npm run build && npx @capawesome/cli apps:bundles:create --app-id 8251f381-4aed-4b20-ac20-a3aad250cbb8 --path dist",
    "capawesome:login": "npx @capawesome/cli login",
    "capawesome:whoami": "npx @capawesome/cli whoami",
    "capawesome:build:ios": "npx @capawesome/cli apps:builds:create --app-id 8251f381-4aed-4b20-ac20-a3aad250cbb8 --platform ios",
    "capawesome:build:android": "npx @capawesome/cli apps:builds:create --app-id 8251f381-4aed-4b20-ac20-a3aad250cbb8 --platform android"
  },
  "dependencies": {
    "@capacitor/android": "^5.7.0",
    "@capacitor/app": "^5.0.7",
    "@capacitor/camera": "^5.0.9",
    "@capacitor/core": "^5.7.0",
    "@capacitor/device": "^5.0.7",
    "@capacitor/filesystem": "^5.2.2",
    "@capacitor/geolocation": "^5.0.7",
    "@capacitor/haptics": "^5.0.8",
    "@capacitor/ios": "^5.7.0",
    "@capacitor/keyboard": "^5.0.9",
    "@capacitor/network": "^5.0.7",
    "@capacitor/push-notifications": "^5.1.1",
    "@capacitor/share": "^5.0.8",
    "@capacitor/splash-screen": "^5.0.7",
    "@capacitor/status-bar": "^5.0.7",
    "@capawesome/capacitor-live-update": "^5.0.0",
    "@emailjs/browser": "^4.4.1",
    "@headlessui/react": "^1.7.18",
    "@stripe/react-stripe-js": "^2.5.1",
    "@stripe/stripe-js": "^3.0.7",
    "@supabase/supabase-js": "^2.39.3",
    "cordova-plugin-purchase": "^13.11.0",
    "date-fns": "^3.3.1",
    "emoji-picker-react": "^4.8.0",
    "framer-motion": "^11.0.8",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.344.0",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-draggable": "^4.4.6",
    "react-leaflet": "^4.2.1",
    "react-player": "^2.16.0",
    "react-router-dom": "^6.22.3",
    "react-virtualized": "^9.22.5",
    "recharts": "^2.12.3",
    "swiper": "^11.0.7"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.7.0",
    "@capawesome/cli": "^3.10.0",
    "@eslint/js": "^9.9.1",
    "@types/leaflet": "^1.9.8",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-virtualized": "^9.21.29",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-pwa": "^0.19.2",
    "workbox-window": "^7.0.0"
  }
}
